-- Crear tabla
CREATE TABLE INVENTARIO (
    ID_INVENTARIO INT NOT NULL,
    DESCRIPCION VARCHAR(200),
    COSTO FLOAT,
    PRECIO FLOAT
);

-- Crear procedimiento almacenado
DELIMITER $$
CREATE PROCEDURE insprocedure(
    IN PIDINVENTARIO INT,
    IN PDESCRIPCION VARCHAR(200),
    IN PCOSTO FLOAT,
    IN PPRECIO FLOAT
)
BEGIN
    INSERT INTO INVENTARIO (ID_INVENTARIO, DESCRIPCION, COSTO, PRECIO)
    VALUES (PIDINVENTARIO, PDESCRIPCION, PCOSTO, PPRECIO);
END$$
DELIMITER ;
