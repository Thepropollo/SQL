CREATE TABLE INVENTARIO
(
ID_INVENTARIO INT NOT NULL,
DESCRIPCION varchar2(200),
CANTIDAD INT,
PRECIO FLOAT,
FECHA DATE
);


create or replace PROCEDURE INSPROCEDURE(PIDINVENTARIO IN INVENTARIO.ID_INVENTARIO%TYPE, PDESCRIPCION IN INVENTARIO.DESCRIPCION%TYPE, 
PCANTIDAD IN INVENTARIO.CANTIDAD%TYPE, PPRECIO IN INVENTARIO.PRECIO%TYPE, PFECHA IN INVENTARIO.FECHA%TYPE)
AS
BEGIN 
INSERT INTO INVENTARIO VALUES (PIDINVENTARIO,PDESCRIPCION,PCANTIDAD,PPRECIO,PFECHA);
END insprocedure;
